{
    "compilerOptions": {
        "target": "ES5",
		"module": "commonjs",
		"skipLibCheck": false,
        "noImplicitAny": false,
        "removeComments": true,
		"preserveConstEnums": true,
		"moduleResolution": "node",
		"sourceMap": true,
		"typeRoots": [
            "node_modules/@types"
		],
		"lib": [
            "es6",
            "dom"
        ]
    },
    "include": [
        "src/*.ts"
	],
	"compileOnSave": false,
    "buildOnSave": false
}
